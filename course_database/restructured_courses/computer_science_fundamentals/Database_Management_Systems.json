  "quiz": {
    "beginner": [
      {
        "question": "Which of the following is a primary feature of relational databases?",
        "options": [
          "Hierarchical data storage",
          "Schema-less data management",
          "Table-based organization",
          "Graph structures"
        ],
        "correct_option": 2,
        "hint": "Relational databases organize data into rows and columns."
      },
      {
        "question": "What is the main purpose of normalization in database design?",
        "options": [
          "To improve data redundancy",
          "To speed up data retrieval",
          "To improve data integrity",
          "To increase storage capacity"
        ],
        "correct_option": 2,
        "hint": "Normalization aims to eliminate data anomalies by organizing data efficiently."
      }
    ],
    "intermediate": [
      {
        "question": "Which SQL statement is used to ensure that a transaction is saved to the database?",
        "options": [
          "ROLLBACK",
          "COMMIT",
          "SAVEPOINT",
          "SET TRANSACTION"
        ],
        "correct_option": 1,
        "hint": "This command is used to make the changes made by a transaction permanent."
      }
    ],
    "advanced": [
      {
        "question": "Which of the following is a key characteristic of NoSQL databases?",
        "options": [
          "Fixed schema",
          "ACID transactions",
          "Horizontal scalability",
          "Table-based data storage"
        ],
        "correct_option": 2,
        "hint": "NoSQL databases are known for their ability to handle large-scale data across distributed systems."
